}
.Repair_time{
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #f5f5f5;
}
.Repair_address{
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
