  justify-content: flex-end;
  padding: 0 20rpx;
  height: 88rpx;
  background-color: #fff;
  font-size: 28rpx;
}
.btn_cancel{
  margin-top: 16rpx;
  margin-right: 20rpx;
  width: 182rpx;
  height: 56rpx;
  border: 2rpx solid;
  border-radius: 50rpx;
  line-height: 56rpx;
  text-align: center;
}
.btn_complete{
  margin-top: 16rpx;
  width: 182rpx;
  height: 56rpx;
  border-radius: 50rpx;
  line-height: 56rpx;
  text-align: center;
  background-color: #2269d4;
  color: #fff;
}
