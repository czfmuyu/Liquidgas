<view class="InformationList" bindtap="BindingNumber">
  <view class="InformationList_left">
    <image src="../../imgs/binding.png" />
    <view class="InformationList_text">
      <text>绑定用气编号</text>
    </view>
  </view>
  <image style='width:35rpx;height:35rpx;' src='../../imgs/dayu.png'></image>
</view>
<view class="InformationList" style="margin-top: 11rpx; border-bottom: 2rpx solid #f2f2f2;" bindtap="AccountSecurity">
  <view class="InformationList_left">
    <image src="../../imgs/security.png" />
    <view class="InformationList_text">
      <text>账号安全</text>
    </view>
  </view>
  <image style='width:35rpx;height:35rpx;' src='../../imgs/dayu.png'></image>
</view>

<view class="InformationList" bindtap="Cancellation">
  <view class="InformationList_left">
    <image src="../../imgs/quit.png" />
    <view class="InformationList_text">
      <text>退出登录</text>
    </view>
  </view>
  <image style='width:35rpx;height:35rpx;' src='../../imgs/dayu.png'></image>
</view>