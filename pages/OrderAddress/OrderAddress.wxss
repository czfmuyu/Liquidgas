/* pages/OrderAddress/OrderAddress.wxss */
@import "../../lib/weui.wxss";
page{
    background-color: #f5f5f5;
}
.header{
    height: 202rpx;
    padding: 0 18rpx;
    display: flex;
    background-color: #fff;
    /* border-bottom:2rpx solid #eee; */
}
.header_left{
    width: 33rpx;
    height: 202rpx;
}
.header_left>image{
    width: 33rpx;
    height: 44rpx;
    margin-top: 102rpx;
}
.header_right{
    width: 666rpx;
    height: 202rpx;
}
.header_right_top{
    width: 666rpx;
    height: 84rpx;
    display: flex;
    justify-content: space-between;
    line-height: 84rpx;
    font-size: 36rpx;
    padding: 0 12rpx;
}
.header_right_bottom{
    width: 666rpx;
    height: 118rpx;
    font-size: 32rpx;
    padding: 0 12rpx;
}
.content{
    height: 88rpx;
    padding: 0 18rpx;
    display: flex;
    background-color: #fff;
}
.content_middle{
    width: 642rpx;
    height: 87rpx;
    display: flex;
    justify-content: space-between;
    line-height: 87rpx;
    padding: 0 14rpx;
}
.hidden_header{
    height: 202rpx;
    padding: 0 18rpx;
    background-color: #fff;
    /* border-bottom:2rpx solid #eee; */
    display: flex;
    /* line-height: 202rpx; */
}
.hidden_header_left>image{
    margin-top: 32rpx;
    width: 632rpx;
    height: 133rpx;
}
.hidden_header_right>image{
    margin-top: 76rpx;
    width: 50rpx;
    height: 50rpx;
}
/* 新增地址弹框 */
.show-btn {
    margin-top: 100rpx;
    color: #22cc22;
  }
  
  .modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
  }
  
  .modal-dialog {
    width: 680rpx;
    overflow: hidden;
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 9999;
    background: #f9f9f9;
    margin: -180rpx 34rpx;
    border-radius: 36rpx;
  }
  .modal-title {
    /* padding-top: 42rpx; */
    font-size: 36rpx;
    color: #030303;
    text-align: center;
  }
  .btn-Photograph{
    width: 680rpx;
    height: 81rpx;
    text-align: center;
    line-height: 89rpx;
    color: #666;
    background-color: #eeeeee;
    font-size: 46rpx;
    font-weight: 900;
  }
  .user{
    margin: 0 auto;
    width: 620rpx;
    height: 70rpx;
    display:flex;
    line-height: 70rpx;
}
.user_left{
    width: 160rpx;
    height: 70rpx;
    color: #aaaaaa;
    text-align: center;
}
.user_right{
    width: 480rpx;
    height: 70rpx;
}
.user_right>input{
    width: 440rpx;
    height: 70rpx;
    border-bottom: 2rpx solid #aaaaaa;
    font-size: 32rpx;
    padding-left: 28rpx;
}
  .modal-ipt{
    padding-bottom: 36rpx;
    color: #c7c7cc;
  }
  .modal-ipt>input{
      width: 477rpx;
      height: 50rpx;
      margin: 0 auto;
      border:2rpx solid;
      padding-left: 10rpx;
  }
  .modal-footer {
    display: flex;
    flex-direction: row;
    height: 86rpx;
    border-top: 1px solid #dedede;
    font-size: 34rpx;
    line-height: 86rpx;
  }
  
  .btn-cancel {
    width: 50%;
    color: #007aff;
    text-align: center;
    border-right: 1px solid #dedede;
  }
  .btn-confirm {
    width: 50%;
    color: #007aff;
    text-align: center;
  }
  .btn-Number{
    width: 680rpx;
    height: 89rpx;
    text-align: center;
    line-height: 89rpx;
    border-bottom: 2rpx solid;
  }
  /* 预定时间弹框 */
.weui-cell__hd{
    text-align: center;
}
.content_left{
    width: 33rpx;
    height: 43rpx;
    position: absolute;
    top: 22rpx;
    left: 20rpx;
}
.content_right{
    position: absolute;
    top: 0rpx;
    right: 18rpx;
    height: 87rpx;
    line-height: 87rpx;
    color: #cdcdcd;
    font-size: 62rpx;
}
/* 商品选择 */
.goods{
    background-color: #fff;
    height: 92rpx;
    padding: 0 20rpx;
    display: flex;
    justify-content: space-between;
    line-height: 92rpx;
}
.goods_right{
    display: flex;
}
.goods_right_text{
    height: 92rpx;
    line-height: 92rpx;
    color: #cdcdcd;
    font-size: 62rpx;
}

/* 商品选择弹框 */
.OptionsBox{
    padding: 0 24rpx;
    height: 100rpx;
    display:flex;
    justify-content: center;
}
.goodsList{
    padding: 0 24rpx;
    height: 108rpx;
    display: flex;
    justify-content: space-between;
    line-height: 108rpx;
    text-align: center;
}
.goodsList_text{
    font-size: 24rpx;
}
.Counter{
    width: 215rpx;
    height: 55rpx;
    border: 2rpx solid #999;
    border-radius: 10rpx;
    margin-top: 22rpx;
    display: flex;
    line-height: 55rpx;
    text-align: center;
}
.Counter_left{
    width: 66rpx;
    height: 55rpx;
    border-right: 2rpx solid #999;
}
.Counter_middle{
    width: 79rpx;
    height: 55rpx;
    
}
.Counter_right{
    border-left: 2rpx solid #999;
    width: 66rpx;
    height: 55rpx;
}
