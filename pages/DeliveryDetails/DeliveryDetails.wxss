
@import "../RepairDetailsComplete/RepairDetailsComplete.wxss";
page{
    background-color: #f5f5f5;
    
}
.header_Order{
  width: 100%;
  height: 150rpx;
  background-color: #fff;
}
.Order{
  padding: 40rpx 38rpx;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
}
.Order_left{
  font-size: 28rpx;
}
.Order_right{
  line-height: 70rpx;
  font-size: 24rpx;
  color: #2269d4;
}
.project{
  margin-top: 8rpx;
  padding: 0 20rpx;
  height: 152rpx;
  background-color: #fff;
}
.project_top{
  height: 58rpx;
  font-size: 28rpx;
  font-weight: 900;
  line-height: 58rpx;
}
.project_bottom{
  height: 92rpx;
  margin-top: 16rpx;
  border-bottom: 2rpx solid #f5f5f5;
}
.project_bottom_box{
  width: 132rpx;
  height: 52rpx;
  font-size: 26rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 52rpx;
  background-color: #2269d4;
  color: #fff;
}
.details{
  background-color: #fff;
  padding: 0 20rpx;
  height: 52rpx;
  font-size: 26rpx;
  line-height: 52rpx;
}
.details_text{
  background-color: #fff;
  padding: 0 20rpx;
  height: 86rpx;
  font-size: 26rpx;
  color: #999;
  border-bottom: 2rpx solid #f5f5f5;
}
.details_time{
  padding: 0 20rpx;
  height: 64rpx;
  display: flex;
  justify-content: flex-start;
  font-size: 26rpx;
  color: #999;
  background-color: #fff;
  line-height: 64rpx;
  border-bottom: 2rpx solid #f5f5f5;
}
.Repair{
  margin-top: 10rpx;
  padding: 0 20rpx;
  height: 86rpx;
  font-size: 28rpx;
  font-weight: 900;
  line-height: 86rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #f5f5f5;
}
.Repair_time{
  display: flex;
  justify-content: space-between;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #f5f5f5;
}
.Repair_address{
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  height: 114rpx;
  font-size: 26rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #f5f5f5;
}
.address{
  width: 108rpx;
  line-height: 114rpx;
}
.address_right{
  margin-top: 28rpx;
  width: 406rpx;
}
.Repair_time_right{
  display: flex;
}
.btn{
  margin-top: 42rpx;
  display: flex;
  justify-content: flex-end;
  padding: 0 20rpx;
  height: 88rpx;
  background-color: #fff;
  font-size: 28rpx;
}
.btn_cancel{
  margin-top: 16rpx;
  margin-right: 20rpx;
  width: 182rpx;
  height: 56rpx;
  border: 2rpx solid;
  border-radius: 50rpx;
  line-height: 56rpx;
  text-align: center;
}
.btn_complete{
  margin-top: 16rpx;
  width: 182rpx;
  height: 56rpx;
  border-radius: 50rpx;
  line-height: 56rpx;
  text-align: center;
  background-color: #2269d4;
  color: #fff;
}


/* 地图 */
.flex-style{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
  }
  .flex-item{
    height: 35px; 
    line-height: 35px;
    text-align: center;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1
  }
  .flex-item.active{
    color:#0091ff;
  }
  #navi_map{
    width: 100%;
    height: 1026rpx;
  }
  .text_box{
    position:absolute;
    height: 90px;
    bottom: 0px;
    left: 0px;
    right: 0px;
  }
  .text_box .text{
    margin: 15px;
  }
  .detail_button{
    position:absolute;
    bottom: 50px;
    right: 10px;
    padding: 3px 5px;
    color: #fff;
    background: #0091ff;
    width:50px;
    text-align:center;
    border-radius:5px;
  }
  .detail_button2{
    position:absolute;
    bottom: 10px;
    right: 10px;
    padding: 3px 5px;
    color: #fff;
    background: #0091ff;
    width:50px;
    text-align:center;
    border-radius:5px;
  }
  
  .bus_box{
    margin: 0 15px;
    padding: 15px 0;
    border-bottom: 1px solid #c3c3c3;
    font-size: 13px;
  }
  .bus_box .bus_item{
    display:inline-block;line-height: 8px;
  }

  .Occupiedlattice{
    width: 100%;
    height: 1026rpx;
    
  }
  .time_box{
    width: 714rpx;
    height: 150rpx;
    background-color: #fff;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
  }
  .time_box_left{
    margin-left: 38rpx;
    height: 75rpx;
    line-height: 75rpx;
  }
  .time_box_right{
    margin-left: 38rpx;
    width: 204rpx;
    height: 75rpx;
    line-height: 75rpx;
  }
  .time_box_right>image{
    width: 204rpx;
    height: 150rpx;
  }
/* 意见弹框 */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}


 .modal-dialog {
  width: 680rpx;
  overflow: hidden;
  position: fixed;
  top: 45%;
  left: 0;
  z-index: 9999;
  background-color: #f9f9f9;
  margin: -180rpx 36rpx;
  border-radius: 36rpx;
}
